<%- include ../partials/header -%>
<div class="container">
    <div class="row">
        <div class="col s12">
            <div class="card z-depth-4">
                <div class="card-content shadow">
                    <table class="table table-striped table-bordered p-3" id="feedbacks" style="width:100%">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Nome</th>
                                <th>Classificação</th>
                                <th>Data Atendimento</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let i=0;i<feedbacks.length;i++) { %>
                                <tr>
                                    <td><%=feedbacks[i].id%></td>
                                    <td><%=feedbacks[i].nome_cliente%></td>
                                    <td><%=feedbacks[i].classificacao%></td>
                                    <td><%=feedbacks[i].data_atendimento.toLocaleString()%></td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include ../partials/footer -%>